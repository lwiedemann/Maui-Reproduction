<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Reproduction.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Reproduction"
    xmlns:pages="clr-namespace:Reproduction.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Reproduction">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Carousel"
        ContentTemplate="{DataTemplate pages:CarouselPage}"
        Route="CarouselPage"/>
</Shell>
